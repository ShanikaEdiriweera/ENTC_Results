{# app/Resources/views/module/viewall.html.twig #}
{% extends 'base.html.twig' %}

{% block title %}
Modules
{% endblock %}

{% block links %}
    <div class="btn-group" >
       <a href="{{url('module_create')}}"> <button type="button" class="btn btn-xs btn-success">Create Module</button></a>
    </div>
{% endblock %}


{% block body %}
<ul>
    {% for module in modules %}
        <li>
        	<a href="{{url('module_results',{'mod_code':module.code})}}">{{ module.code|e }} - {{ module.title|e }} - {{ module.credits|e }} - {{ module.gpa|e }} - {{ module.semId|e }} </a>
        </li>
    {% endfor %}
</ul>

{% endblock %}
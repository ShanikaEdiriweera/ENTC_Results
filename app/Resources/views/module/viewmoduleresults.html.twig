{# app/Resources/views/module/viewmoduleresults.html.twig #}
{% extends 'base.html.twig' %}

{% block title %}
{{code}} - Results
{% endblock %}

{% block body %}
<ul>
    {% for result in results %}
    	{% if code == result.mCode %}
        <li>
        	<a href="{{url('module_results',{'mod_code':result.mCode})}}">{{ result.sId|e }} - {{ result.grade|e }} </a>
        </li>
        {% endif %}
    {% endfor %}
</ul>

{% endblock %}
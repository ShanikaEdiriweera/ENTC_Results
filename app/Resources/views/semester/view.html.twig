{# app/Resources/semester/view.html.twig #}
{% extends 'base.html.twig' %}

{% block title %}
{{semester.name}} Results
{% endblock %}

{% block body %}
<div class="results">
	<ul>
    {% for result in results %}
        <li>
        	{{ result.studentIndex |e }} - {{ result.studentName |e }} - {{ result.gPA}} - {{ result.rank}}
        </li>
    {% endfor %}
	</ul>
	
</div> 
{% endblock %}